{
  "name": "mrbenosborne/super-collection",
  "description": "Enhances the Collection class provided by Laravel.",
  "keywords": [
    "laravel",
    "collection"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Ben Osborne",
      "email": "hello@benosborne.io"
    }
  ],
  "require": {
    "php": ">=7.0|^8.0",
    "illuminate/support": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "SuperCollection\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "SuperCollection\\ServiceProvider"
      ]
    }
  },
  "require-dev": {
    "laravel/framework": "^8.0",
    "phpunit/phpunit": "9.3.x"
  }
}